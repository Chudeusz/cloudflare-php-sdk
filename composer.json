{
  "name": "chudeusz/cloudflare-sdk",
  "description": "PHP binding for v4 of the Cloudflare Client API.",
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "^7.1.1",
    "php": ">=7.2.5",
    "psr/http-message": "~1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "phpmd/phpmd" : "@stable",
    "friendsofphp/php-cs-fixer": "^2.6"
  },
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Kamil Karpi≈Ñski",
      "email": "kamil.karpinski@owncp.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cloudflare\\API\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
        "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
